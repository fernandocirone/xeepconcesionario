@model IEnumerable<Contrato>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreContrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DescripcionContrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PlazoMeses)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreContrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescripcionContrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlazoMeses)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ContratoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ContratoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ContratoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
